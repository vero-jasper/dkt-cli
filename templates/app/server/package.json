{
  "name": "server",
  "version": "0.0.1",
  "description": "a node web server powerd by bucky",
  "scripts": {
    "start": "npm run dev",
    "bucky-dev": "bucky lift -e 'development'",
    "bucky-build": "babel src --out-dir dist --copy-files --ignore statics/*",
    "mix-dev": "bucky-mix -w -r",
    "mix-build": "bucky-mix -p -a production",
    "dev": "run-p -s bucky-dev mix-dev",
    "build-clear": "rm -rf ./dist",
    "build-mix-copy": "cp -r ./src/statics/.mix ./dist/statics",
    "build": "run-s build-clear bucky-build mix-build build-mix-copy",
    "test": "NODE_ENV=testing node ./dist/index.js"
  },
  "license": "ISC",
  "registry": "http://registry.npmjs.lianjia.com:7001/",
  "dependencies": {
    "@lianjia/bucky-core": "0.1.10",
    "@lianjia/koa-oauth": "0.0.3",
    "axios": "^0.18.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "ini": "^1.3.4",
    "jquery": "^3.3.1",
    "koa-compress": "^2.0.0",
    "npm-run-all": "^4.1.2"
  },
  "devDependencies": {
    "@lianjia/bucky-mix": "1.1.7",
    "@lianjia/bucky-cli": "0.1.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "1.12.1"
  }
}
